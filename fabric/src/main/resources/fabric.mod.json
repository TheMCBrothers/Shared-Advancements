{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://themcbrothers.net/mods",
        "sources": "https://github.com/${github_repo}",
        "issues": "https://github.com/${github_repo}/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/${mod_id}.png",
    "environment": "server",
    "entrypoints": {
        "main": [
            "net.themcbrothers.sharedadvancements.SharedAdvancements"
        ]
    },
    "mixins": [
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric-api": "*",
        "minecraft": "1.20.2",
        "java": ">=17"
    }
}
  